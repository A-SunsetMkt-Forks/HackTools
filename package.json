{
  "name": "hacktools",
  "version": "1.0.0",
  "description": "The all in one  browser extension for offensive security professionals",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wxt && npx prettier --write \"{entrypoints,public}/**/*.{js,jsx,ts,tsx,md,json,html,css}\" --config .prettierrc.json",
    "dev:firefox": "wxt -b firefox && npx prettier --write \"{entrypoints,public}/**/*.{js,jsx,ts,tsx,md,json,html,css}\" --config .prettierrc.json",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "prettier": "npx prettier --write \"{entrypoints,public}/**/*.{js,jsx,ts,tsx,md,json,html,css}\" --config .prettierrc.json"
  },
