{
    "name": "hack-tools",
    "version": "0.5.0",
    "description": "The all in one Red team browser extension for web pentesters",
    "main": "index.js",
    "scripts": {
        "zip_chrome": "rm -f './dist/*{.map,.txt}' && zip -r bundle_chrome.zip dist/",
        "zip_moz": " rm -f './dist/*{.map,.txt}' && cd ./dist && zip -r ../bundle_moz.zip * && cd -",
        "zip_all": "npm run  zip_chrome && npm run  zip_moz",
        "build": "tsc --noEmit && vite build",
        "build:watch": "__DEV__=true vite build --watch",
        "build:hmr": "rollup --config utils/reload/rollup.config.ts",
        "wss": "node utils/reload/initReloadServer.js",
        "dev": "npm run build:hmr && (run-p wss build:watch)",
        "test": "jest"
    },
    "keywords": [],
    "author": "Ludovic COULON & Riadh BOUCHAHOUA",
    "license": "MIT",
    "browserslist": [
        ">0.2%",
        "not dead",
        "not op_mini all"
    ],
    "dependencies": {
        "antd": "5.1.0",
        "antd-mask-input": "2.0.7",
        "axios": "^1.3.4",
        "crypto-browserify": "^3.12.0",
